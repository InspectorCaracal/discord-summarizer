# Copy this file to .env and fill this in!
DISCORD_BOT_TOKEN=CHANGE_ME

# The rest of these, either change or delete - the code fills in defaults for them.
DS_COMMAND_PREFIX=!
DS_LOG_LEVEL=INFO
# True: only summarizes conversations in channels you toggle on with !track.
# False: summarizes conversations on ANY channel.
DS_WHITELIST_MODE=true
# LLM API configuration
LLM_API_KEY=CHANGE_ME
LLM_API_BASE_URL=https://api.your-llm-provider.com/v1
LLM_MODEL=gpt-5-mini